# TBO Holidays API Configuration
# Copy this file to .env and update with your actual credentials

# API Base URL (staging/test environment)
TBO_BASE_URL=https://api.tboholidaystaging.com/TBOHolidays_HotelAPI/hotelapi

# Production API URL (uncomment when ready for production)
# TBO_BASE_URL=https://api.tboholidays.com/TBOHolidays_HotelAPI/hotelapi

# API Credentials (obtain from TBO Technology)
TBO_USERNAME=your_api_username
TBO_PASSWORD=your_api_password

# Request Configuration
TBO_TIMEOUT=30000
TBO_RETRIES=3

# Logging
TBO_LOG_LEVEL=info
TBO_LOG_REQUESTS=true
TBO_LOG_RESPONSES=false